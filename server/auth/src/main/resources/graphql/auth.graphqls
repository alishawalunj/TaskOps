type UserDTO {
    id: ID
    userName: String
    email: String
    provider: String
    address: String
    age: Int
    sex: String
}

type JwtAuthResponse {
    accessToken: String
    tokenType: String
}

input LoginDTO {
    email: String!
    password: String!
}

input UserInput {
    userName: String!
    email: String!
    password: String!
    address: String
    age: Int
    sex: String
}

type Mutation {
    registerUser(user: UserInput!): UserDTO
    loginUser(credentials: LoginDTO!): JwtAuthResponse
}
